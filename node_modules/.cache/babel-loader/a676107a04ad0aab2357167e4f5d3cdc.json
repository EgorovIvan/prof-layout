s.totalPrice();\n    },\n\n    addCount(id) {\n      this.cart.map(item => {\n        if (item.id_product === id) {\n          item.quantity++;\n        }\n      });\n      let string = JSON.stringify(this.cart);\n      localStorage.setItem(\"toCart\", string);\n      this.totalPrice();\n    },\n\n    totalPrice() {\n      let sum = 0;\n      this.cart.map(product => {\n        return sum += product.price * product.quantity;\n      });\n      this.cartPrice = sum;\n    },\n\n    render() {\n      this.cart = JSON.parse(localStorage.getItem(\"toCart\"));\n    }\n\n  },\n\n  mounted() {}\n\n};","map":{"version":3,"mappings":"AAWC,OAAOA,WAAP,MAAwB,0BAAxB,C,CACC;;AAED,eAAe;EACdC,IAAI,EAAE,KADQ;EAEdC,UAAU,EAAE;IACXF;EADW,CAFE;;EAKdG,IAAI,GAAG;IACN,OAAO;MACNC,UAAU,EAAE,eADN;MAENC,IAAI,EAAEC,IAAI,CAACC,KAAL,CAAWC,YAAY,CAACC,OAAb,CAAqB,QAArB,CAAX,CAFA;MAGNC,GAAG,EAAE,gBAHC;MAINC,UAAU,EAAE,EAJN;MAKNC,SAAS,EAAE,CALL;MAMNC,KAAK,EAAE;IAND,CAAP;EAQA,CAda;;EAedC,OAAO,EAAE;IAERC,OAAO,CAACC,GAAD,EAAM;MACZ,OAAOC,KAAK,CAACD,GAAD,CAAL,CACLE,IADK,CACAC,MAAK,IAAKA,MAAM,CAACC,IAAP,EADV,EAELC,KAFK,CAECR,KAAI,IAAK;QACfS,OAAO,CAACC,GAAR,CAAYV,KAAZ;QACA,KAAKA,KAAL,GAAa,IAAb;MACA,CALK,CAAP;IAMA,CATO;;IAURW,UAAU,CAACC,OAAD,EAAU;MACnB,IAAIC,cAAa,GAAIpB,IAAI,CAACC,KAAL,CAAWC,YAAY,CAACC,OAAb,CAAqB,QAArB,CAAX,CAArB;MACA,IAAIkB,IAAG,GAAID,cAAc,CAACC,IAAf,CAAqBC,IAAG,IAAKH,OAAO,CAACI,UAAR,KAAuBD,IAAI,CAACC,UAAzD,CAAX;;MACA,IAAIF,IAAJ,EAAU;QACTA,IAAI,CAACG,QAAL;MACD,CAFA,MAEO;QACN,MAAMC,IAAG,GAAIC,MAAM,CAACC,MAAP,CAAc;UAACH,QAAQ,EAAE;QAAX,CAAd,EAA6BL,OAA7B,CAAb;QACAC,cAAc,CAACQ,IAAf,CAAoBH,IAApB;MACD;;MACA,IAAII,MAAK,GAAI7B,IAAI,CAAC8B,SAAL,CAAeV,cAAf,CAAb;MACAlB,YAAY,CAAC6B,OAAb,CAAqB,QAArB,EAA+BF,MAA/B;MACA,KAAKG,MAAL;IACA,CAtBO;;IAuBRC,KAAK,GAAG;MACP,IAAIC,UAAS,GAAIC,QAAQ,CAACC,gBAAT,CAA0B,kBAA1B,CAAjB;;MACA,KAAK,IAAIC,IAAT,IAAiBH,UAAjB,EAA6B;QAC5BG,IAAI,CAACC,MAAL;MACD;;MACA,KAAKvC,IAAL,GAAY,EAAZ;MACAG,YAAY,CAACqC,UAAb,CAAwB,QAAxB;MACA,KAAKC,UAAL;IACA,CA/BO;;IAgCRC,UAAU,CAACC,EAAD,EAAK;MACd,KAAK3